{
    "hybrid": {
        "namespace": "hybrid-bar",
        "features": ["hyprland"],
        "r": 10,
        "g": 10,
        "b": 10,
        "a": 1.0
    },
    "right-label_volume": {
        "text": "VOL: ",
        "command": "(pamixer --get-volume-human)",
        "update_rate": 10000
    },
    "right-label_volume_separator": {
        "text": "  |  "
    },
    "right-label_backlight": {
        "command": "~/.config/HybridBar/update_backlight",
        "update_rate": 10000
  },
    "right-label_backlight_separator": {
        "text": "  |  "
    },
    "right-label_battery": {
        "command": "~/.config/HybridBar/update_battery",
        "update_rate": 60000
  },
    "right-label_battery_separator": {
        "text": "  |  "
    },
    "right-label_date": {
        "command": "date '+[%d/%m-%y] [%R]'",
        "update_rate": 60000
    }

}
